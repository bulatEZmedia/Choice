<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentStart">

    <fragment
        android:id="@+id/fragmentStart"
        android:name="com.example.mychoiceai.FragmentStart"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_fragmentStart_to_fragmentGender"
            app:destination="@id/fragmentGender" />
    </fragment>
    <fragment
        android:id="@+id/fragmentGender"
        android:name="com.example.mychoiceai.FragmentGender"
        android:label="fragment_gender"
        tools:layout="@layout/fragment_gender" >
        <action
            android:id="@+id/action_fragmentGender_to_fragmentBirthday"
            app:destination="@id/fragmentBirthday" />
    </fragment>
    <fragment
        android:id="@+id/fragmentBirthday"
        android:name="com.example.mychoiceai.FragmentBirthday"
        android:label="fragment_birthday"
        tools:layout="@layout/fragment_birthday" >
        <action
            android:id="@+id/action_fragmentBirthday_to_fragmentLogin"
            app:destination="@id/fragmentLogin" />
        <argument android:name="gender"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLogin"
        android:name="com.example.mychoiceai.FragmentLogin"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentName"
            app:destination="@id/fragmentName" />
        <argument
            android:name="b"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/fragmentName"
        android:name="com.example.mychoiceai.FragmentName"
        android:label="fragment_name"
        tools:layout="@layout/fragment_name" >
        <action
            android:id="@+id/action_fragmentName_to_fragmentEmail"
            app:destination="@id/fragmentEmail" />
    </fragment>
    <fragment
        android:id="@+id/fragmentEmail"
        android:name="com.example.mychoiceai.FragmentEmail"
        android:label="fragment_email"
        tools:layout="@layout/fragment_email" >
        <action
            android:id="@+id/action_fragmentEmail_to_fragmentPassword"
            app:destination="@id/fragmentPassword" />
    </fragment>
    <fragment
        android:id="@+id/fragmentPassword"
        android:name="com.example.mychoiceai.FragmentPassword"
        android:label="fragment_password"
        tools:layout="@layout/fragment_password" >
        <argument
            android:name="gender"
            app:argType="string" />
    </fragment>
</navigation>