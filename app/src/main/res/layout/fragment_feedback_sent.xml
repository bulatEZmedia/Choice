<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentFeedbackSent"
    android:background="@drawable/background">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/then49"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="339dp"
        android:layout_height="63dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#DEFA2C"
        android:text="Готово"
        android:textAllCaps="false"
        android:textColor="#192126"
        android:textSize="16sp"
        android:textStyle="bold"
        app:cornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView144" />

    <ImageView
        android:id="@+id/imageView84"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:src="@drawable/feedbacksentskala"
        app:layout_constraintEnd_toEndOf="@+id/imageView83"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView83"
        app:layout_constraintTop_toTopOf="@+id/imageView83" />

    <ImageView
        android:id="@+id/imageView83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:src="@drawable/feedbacksent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView143" />

    <ImageView
        android:id="@+id/imageView82"
        android:layout_width="26dp"
        android:layout_height="33dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="76dp"
        android:src="@drawable/vectorchoice"
        app:layout_constraintEnd_toEndOf="@+id/textView143"
        app:layout_constraintTop_toTopOf="@+id/textView143" />

    <TextView
        android:id="@+id/textView143"
        android:layout_width="243dp"
        android:layout_height="73dp"
        android:layout_marginTop="64dp"
        android:text="Проблема успешно отправлена!"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView144"
        android:layout_width="302dp"
        android:layout_height="70dp"
        android:layout_marginTop="52dp"
        android:text="Спасибо, что помогаете нам стать лучше"
        android:textColor="#DEFA2C"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.289"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView83" />
</androidx.constraintlayout.widget.ConstraintLayout>